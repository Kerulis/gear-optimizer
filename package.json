{
  "homepage": "https://gmiclotte.github.io/gear-optimizer",
  "name": "gear-optimizer",
  "version": "0.3.31",
  "private": true,
  "dependencies": {
    "caniuse-lite": "^1.0.30001008",
    "gh-pages": "^2.1.1",
    "react": "^16.11.0",
    "react-cookie-banner": "^4.0.0",
    "react-dom": "^16.11.0",
    "react-ga": "^2.7.0",
    "react-modal": "^3.11.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.0.1",
    "react-tooltip": "^3.11.1",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.3",
    "worker-loader": "^2.0.0"
  },
  "scripts": {
    "git-info": "echo export default \"{\\\"logMessage\\\": \\\"$(git log -1 --oneline)\\\"}\"  > src/_git_commit.js",
    "start": "react-app-rewired start",
    "build": "npm run release-it; npm run git-info; react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "release-it": "release-it; echo export default \"{$(grep version -m1 package.json)}\"  > src/_version.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "release-it": {
    "npm": {
      "release": false
    }
  },
  "devDependencies": {
    "react-app-rewired": "^2.1.4",
    "release-it": "^12.4.3"
  }
}
